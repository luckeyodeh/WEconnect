{
  "name": "weconnect",
  "version": "1.0.0",
  "description": "Weconnect provides a platform that brings businesses and individuals together. This platform creates awareness for businesses and gives the users the ability to write reviews about the businesses they have interacted with.",
  "main": "app.js",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.16.2",
    "jade": "^1.11.0",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.5",
    "morgan": "^1.9.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.36.1",
    "sequelize-cli": "^4.0.0",
    "serve-favicon": "^2.4.5",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "mocha": "^5.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.17.1",
    "nyc": "^11.4.1",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "start": "nodemon --exec babel-node --presets es2015 ./bin/www",
    "start:dev": "nodemon .bin/www",
    "build": "babel server -d ./dist --ignore server/dist",
    "unmigrate": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate:undo:all",
    "seed": "node ./node_modules/sequelize-cli/lib/sequelize db:seed:all",
    "migrate": "npm run unmigrate && node ./node_modules/sequelize-cli/lib/sequelize db:migrate && npm run seed",
    "heroku-postbuild": "npm run migrate && npm run build",
    "postinstall": "npm run build",
    "test": "nyc --reporter=html --reporter=text mocha server/tests/ --exit --compilers js:babel-register",
    "test-watch": "nodemon --exec \"npm test\"",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luckeyodeh/WEconnect.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luckeyodeh/WEconnect/issues"
  },
  "homepage": "https://github.com/luckeyodeh/WEconnect#readme"
}
